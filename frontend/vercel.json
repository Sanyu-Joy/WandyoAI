{
  "version": 2,
  "name": "wandyoai-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@wandyoai-backend-url"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "@wandyoai-backend-url"
    }
  }
}